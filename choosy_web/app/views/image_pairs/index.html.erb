<p id="notice"><%= notice %></p>

<h1>Listing Image Pairs</h1>

<table>
  <thead>
    <tr>
      <th>Votes first</th>
      <th>Votes second</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @image_pairs.each do |image_pair| %>
      <tr>
        <td><%= image_pair.votes_first %></td>
        <td><%= image_pair.votes_second %></td>
        <td><%= image_pair.user %></td>
        <td><%= link_to 'Show', image_pair %></td>
        <td><%= link_to 'Edit', edit_image_pair_path(image_pair) %></td>
        <td><%= link_to 'Destroy', image_pair, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Image pair', new_image_pair_path %>
